{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es6",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "removeComments": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "strictPropertyInitialization": false
  },
  "include": ["src/**/*"]
}
